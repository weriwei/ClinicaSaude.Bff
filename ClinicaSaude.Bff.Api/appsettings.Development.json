{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "NewRelic.LogEnrichers.Serilog" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [ "WithNewRelicLogsInContext" ],
    "Properties": {
      "service": "clinicasaude-bff",
      "environment": "dev"
    },
    "WriteTo": {
      "Console": {
        "Name": "Console",
        "Args": {
          "formatter": "NewRelic.LogEnrichers.Serilog.NewRelicFormatter, NewRelic.LogEnrichers.Serilog"
        }
      }
    }
  },
  "Authentication": {
    "Authority": "https://hefesto-dev.pottencial.com.br",
    "Audience": "clinicasaude-bff-api"
  }
}
